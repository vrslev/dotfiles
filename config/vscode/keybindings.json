[
  {
    // to ←
    "key": "ctrl+0",
    "when": "!sideBarFocus",
    "command": "workbench.action.focusSideBar"
  },
  {
    // from →
    "key": "ctrl+0",
    "when": "sideBarFocus",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  {
    "key": "ctrl+shift+m",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "shift+enter",
    "when": "editorFocus",
    "command": "editor.action.openLink"
  },
  {
    "key": "cmd+k a",
    "command": "explorer.newFile"
  },
  {
    "key": "cmd+k shift+a",
    "command": "explorer.newFolder"
  },
  {
    "key": "cmd+m",
    "command": "-continue.focusContinueInput"
  },
  {
    "key": "shift+cmd+m",
    "command": "-continue.focusContinueInputWithoutClear"
  },
  {
    "key": "cmd+i",
    "command": "-continue.quickEdit"
  },
  {
    "key": "f3",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "f2",
    "command": "-editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+alt+o",
    "command": "workbench.action.addRootFolder"
  },
  {
    "key": "cmd+k cmd+w",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.files.action.collapseExplorerFolders",
        "workbench.action.closeAllEditors"
      ]
    }
  },
  {
    "key": "ctrl+shift+t",
    "when": "editorLangId == 'python'",
    "command": "runCommands",
    "args": {
      "commands": [

        "workbench.action.files.save",
        "workbench.action.terminal.newInActiveWorkspace",
        "workbench.action.terminal.toggleTerminal",
        {
          "command": "workbench.action.terminal.sendSequence",
          "args": {
            "text": "${userHome}/code/auto-typing-final/.venv/bin/auto-typing-final ${file}\u000Dexit\u000D"
          }
        }
      ]
    }
  },
  {
    "key": "ctrl+shift+u",
    "when": "editorLangId == 'python'",
    "command": "runCommands",
    "args": {
      "commands": [

        "workbench.action.files.save",
        "workbench.action.terminal.newInActiveWorkspace",
        {
          "command": "workbench.action.terminal.sendSequence",
          "args": {
            "text": "${userHome}/code/auto-typing-final/.venv/bin/auto-typing-final ${file}\u000D"
          }
        }
      ]
    }
  }
]
