[
  {
    // to ←
    "key": "ctrl+0",
    "when": "!sideBarFocus",
    "command": "workbench.action.focusSideBar"
  },
  {
    // from →
    "key": "ctrl+0",
    "when": "sideBarFocus",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  {
    "key": "ctrl+shift+m",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "shift+enter",
    "when": "editorFocus",
    "command": "editor.action.openLink"
  }
]
