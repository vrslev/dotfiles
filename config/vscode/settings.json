{
  "$schema": "vscode://schemas/settings/user",
  "[html][javascript][json][jsonc][markdown][typescript][typescriptreact][vue][yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "codeium.enableCodeLens": false,
  "codeium.enableConfig": {
    "*": true,
    "markdown": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "dotenv.enableAutocloaking": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.scrollbar.horizontalScrollbarSize": 7,
  "editor.scrollbar.verticalScrollbarSize": 7,
  "editor.stickyScroll.enabled": true,
  "emmet.includeLanguages": {
    "jinja-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "errorLens.enabledDiagnosticLevels": ["error", "warning"],
  "errorLens.messageBackgroundMode": "message",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "files.associations": {
    ".env*": "dotenv",
    ".fish_profile": "fish",
    "Brewfile-*": "ruby"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 300,
  "files.exclude": {
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.ruff_cache": true,
    "**/__pycache__": true,
    "**/.coverage*": true,
    "**/coverage.xml": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.pullBranch": "always",
  "githubPullRequests.pushBranch": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "markdown.preview.typographer": true,
  "mypy-type-checker.importStrategy": "fromEnvironment",
  "mypy-type-checker.preferDaemon": true,
  "mypy-type-checker.reportingScope": "workspace",
  "pylint.importStrategy": "fromEnvironment",
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.gotoDefinitionInStringLiteral": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.useLibraryCodeForTypes": false,
  "python.diagnostics.sourceMapsEnabled": true,
  "python.testing.pytestEnabled": true,
  "redhat.telemetry.enabled": false,
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "scm.diffDecorations": "gutter",
  "security.workspace.trust.enabled": false,
  "terminal.external.osxExec": "Wezterm.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.tabs.hideCondition": "never",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.autoDetectColorScheme": true,
  "window.newWindowDimensions": "maximized",
  "workbench.activityBar.location": "top",
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.settings.openDefaultSettings": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false
}
