[
  {
    // to ←
    "key": "ctrl+0",
    "when": "!sideBarFocus",
    "command": "workbench.action.focusSideBar"
  },
  {
    // from →
    "key": "ctrl+0",
    "when": "sideBarFocus",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  {
    "key": "ctrl+shift+n",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "shift+enter",
    "when": "editorFocus",
    "command": "editor.action.openLink"
  },
  {
    "key": "cmd+k a",
    "command": "explorer.newFile"
  },
  {
    "key": "cmd+k shift+a",
    "command": "explorer.newFolder"
  },
  {
    "key": "cmd+m",
    "command": "-continue.focusContinueInput"
  },
  {
    "key": "shift+cmd+m",
    "command": "-continue.focusContinueInputWithoutClear"
  },
  {
    "key": "cmd+i",
    "command": "-continue.focusEdit"
  },
  {
    "key": "f3",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "f2",
    "command": "-editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+alt+o",
    "command": "workbench.action.addRootFolder"
  },
  {
    "key": "cmd+k cmd+w",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.files.action.collapseExplorerFolders",
        "workbench.action.closeAllEditors"
      ]
    }
  },
  {
    "key": "ctrl+alt+`",
    "command": "workbench.action.terminal.newInActiveWorkspace",
    "when": "terminalProcessSupported"
  },
  {
    "key": "cmd+ctrl+enter",
    "command": "runCommands",
    "args": {
      "commands": [
        "git.commit",
        "git.push"
      ]
    },
    "when": "workbench.scm.active"
  },
  {
    "key": "cmd+i",
    "command": "-inlineChat.start",
    "when": "editorFocus && inlineChatHasEditsAgent && inlineChatPossible && !editorReadonly && !editorSimpleInput || editorFocus && inlineChatHasProvider && inlineChatPossible && !editorReadonly && !editorSimpleInput"
  },
  {
    "key": "ctrl+alt+z",
    "command": "workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+shift+`",
    "command": "-workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+,",
    "command": "workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
  {
    "key": "ctrl+.",
    "command": "workbench.action.navigateForward",
    "when": "canNavigateForward"
  },
  {
    "key": "cmd+shift+i",
    "command": "continue.focusEdit"
  },
  {
    "key": "cmd+k cmd+p",
    "command": "workbench.action.tasks.runTask",
    "args": "Python — auto-type-annotate",
    "when": "editorTextFocus && editorLangId == python"
  }
]
